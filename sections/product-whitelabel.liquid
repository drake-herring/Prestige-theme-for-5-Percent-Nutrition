        <div class="product-thumbnail-images whitelabel-thumb owl-carousel owl-theme hide-mob hide-tab" data-template-suffix="{{ product.template_suffix }}">
        {% assign maxThumbnails = 10 %}
        {% for image in product.images %}
          {% if forloop.index <= maxThumbnails %}
            {% if forloop.index == 2 %}
              {% if product.metafields.meta.video != blank %}
                <div class="item">
                    <div class="product-thumbnail-image videoContainer">
                      <a class="product-video" href="{{ product.metafields.meta.video}}"></a>
                      <img loading="lazy" src="{{ product.images[0].src | img_url: 'master' }}" alt="{{ product.images[0].alt | escape }}" class="product-thumbnail-image__img" data-main-image="{{ product.images[0].src | img_url: 'master' }}">
