{%- assign main_menu_handle = section.settings.primary_menu | default: 'main-menu' -%}
{%- assign main_menu = linklists[main_menu_handle] -%}

<section id="sidebar-menu" class="SidebarMenu Drawer Drawer--small Drawer--fromLeft mobile-sidenav" aria-hidden="true" data-section-id="{{ section.id }}" data-section-type="sidebar-menu">
    <header class="Drawer__Header" data-drawer-animated-left>    
      <div style="width:135px; background: transparent">
        <img loading="lazy" src="{{ mobile_logo }}"/>
      </div>
      <button class="Drawer__Close close-navbar-mobile Icon-Wrapper--clickable" data-action="close-drawer" data-drawer-id="sidebar-menu" aria-label="{{ 'header.navigation.close_sidebar' | t }}">
        {%- include 'icon' with 'close' -%}
      </button>
    </header>

    <div class="Drawer__Content">
      <div class="Drawer__Main Drawer__Main--sidenav" data-drawer-animated-left data-scrollable>
        <div class="Drawer__Container Drawer__Container--sidenav">
          <nav class="SidebarMenu__Nav SidebarMenu__Nav--primary" aria-label="{{ 'header.navigation.sidebar_title' | t }}">
            {%- comment %}<locksmith:2567>{% endcomment -%}
              {%- assign locksmith_181d_forloop__size = 0 %}{%- for link in main_menu.links -%}{% capture var %}{% render 'locksmith-variables', scope: 'subject', subject: link, subject_parent: main_menu, variable: 'transparent' %}{% endcapture %}{% if var == 'true' %}{% assign locksmith_181d_forloop__size = locksmith_181d_forloop__size | plus: 1 %}{% endif %}{% endfor %}{% assign locksmith_181d_forloop__index = nil -%}
            {%- comment %}</locksmith:2567>{% endcomment -%}
            {%- for link in main_menu.links -%}
              {%- comment %}<locksmith:823e>{% endcomment -%}
                {%- capture var %}{% render 'locksmith-variables', scope: 'subject', subject: link, subject_parent: main_menu, variable: 'transparent' %}{% endcapture %}{% if var == "true" %}{% if locksmith_181d_forloop__index == nil %}{% assign locksmith_181d_forloop__index = 1 %}{% assign locksmith_181d_forloop__index0 = 0 %}{% else %}{% assign locksmith_181d_forloop__index = locksmith_181d_forloop__index | plus: 1 %}{% assign locksmith_181d_forloop__index0 = locksmith_181d_forloop__index0 | plus: 1 %}{% endif %}{% if locksmith_181d_forloop__index == 1 %}{% assign locksmith_181d_forloop__first = true %}{% else %}{% assign locksmith_181d_forloop__first = false %}{% endif %}{% if locksmith_181d_forloop__index == locksmith_181d_forloop__size %}{% assign locksmith_181d_forloop__last = true %}{% else %}{% assign locksmith_181d_forloop__last = false %}{% endif %}{% assign locksmith_181d_forloop__rindex = locksmith_181d_forloop__size | minus: locksmith_181d_forloop__index | minus: 1 %}{% assign locksmith_181d_forloop__rindex0 = locksmith_181d_forloop__size | minus: locksmith_181d_forloop__index0 | minus: 1 %}{% else %}{% continue %}{% endif -%}
              {%- comment %}</locksmith:823e>{% endcomment -%}
              <div class="Collapsible">
                {%- if link.links != blank -%}
                  <button class="Collapsible__Button Heading u-h6 Heading--sidenav text-uppercase heading-main" data-action="toggle-collapsible" {% if link.links != blank %}aria-expanded="false"{% endif %}>
                    {{- link.title | escape -}} <span class="Collapsible__Plus"></span>
                  </button>

                  <div class="Collapsible__Inner">
                    <div class="Collapsible__Content">
                      {%- comment %}<locksmith:94a7>{% endcomment -%}
                        {%- assign locksmith_ab1a_forloop__size = 0 %}{%- for sub_link in link.links -%}{% capture var %}{% render 'locksmith-variables', scope: 'subject', subject: sub_link, subject_parent: link, variable: 'transparent' %}{% endcapture %}{% if var == 'true' %}{% assign locksmith_ab1a_forloop__size = locksmith_ab1a_forloop__size | plus: 1 %}{% endif %}{% endfor %}{% assign locksmith_ab1a_forloop__index = nil -%}
                      {%- comment %}</locksmith:94a7>{% endcomment -%}
                      {%- for sub_link in link.links -%}
                        {%- comment %}<locksmith:c7d0>{% endcomment -%}
                          {%- capture var %}{% render 'locksmith-variables', scope: 'subject', subject: sub_link, subject_parent: link, variable: 'transparent' %}{% endcapture %}{% if var == "true" %}{% if locksmith_ab1a_forloop__index == nil %}{% assign locksmith_ab1a_forloop__index = 1 %}{% assign locksmith_ab1a_forloop__index0 = 0 %}{% else %}{% assign locksmith_ab1a_forloop__index = locksmith_ab1a_forloop__index | plus: 1 %}{% assign locksmith_ab1a_forloop__index0 = locksmith_ab1a_forloop__index0 | plus: 1 %}{% endif %}{% if locksmith_ab1a_forloop__index == 1 %}{% assign locksmith_ab1a_forloop__first = true %}{% else %}{% assign locksmith_ab1a_forloop__first = false %}{% endif %}{% if locksmith_ab1a_forloop__index == locksmith_ab1a_forloop__size %}{% assign locksmith_ab1a_forloop__last = true %}{% else %}{% assign locksmith_ab1a_forloop__last = false %}{% endif %}{% assign locksmith_ab1a_forloop__rindex = locksmith_ab1a_forloop__size | minus: locksmith_ab1a_forloop__index | minus: 1 %}{% assign locksmith_ab1a_forloop__rindex0 = locksmith_ab1a_forloop__size | minus: locksmith_ab1a_forloop__index0 | minus: 1 %}{% else %}{% continue %}{% endif -%}
                        {%- comment %}</locksmith:c7d0>{% endcomment -%}
                        <div class="Collapsible {% if sub_link.links != blank %} has-subtitle {% endif %}">
                          {%- if sub_link.links != blank -%}
                            {% comment %} --- Sub Title on "Supplements" submenu --- {% endcomment %}
                            <h4 class="subtitle">{{ sub_link.title | escape }}</h4>
                          
                            {% comment %} <button class="Collapsible__Button Heading Heading--sidenav Text--subdued Link--primary u-h7 heading-secondary" data-action="toggle-collapsible" {% if sub_link.links != blank %}aria-expanded="false"{% endif %}>
                              {{- sub_link.title | escape -}} <span class="Collapsible__Plus"></span>
                            </button> {% endcomment %}

                            {% comment %} <div class="Collapsible__Inner"> {% endcomment %}
                              {% comment %} <div class="Collapsible__Content"> {% endcomment %}
                                <ul class="Linklist">
                                  {%- comment %}<locksmith:c695>{% endcomment -%}
                                    {%- assign locksmith_b670_forloop__size = 0 %}{%- for sub_sub_link in sub_link.links -%}{% capture var %}{% render 'locksmith-variables', scope: 'subject', subject: sub_sub_link, subject_parent: sub_link, variable: 'transparent' %}{% endcapture %}{% if var == 'true' %}{% assign locksmith_b670_forloop__size = locksmith_b670_forloop__size | plus: 1 %}{% endif %}{% endfor %}{% assign locksmith_b670_forloop__index = nil -%}
                                  {%- comment %}</locksmith:c695>{% endcomment -%}
                                  {%- for sub_sub_link in sub_link.links -%}
                                    {%- comment %}<locksmith:08dd>{% endcomment -%}
                                      {%- capture var %}{% render 'locksmith-variables', scope: 'subject', subject: sub_sub_link, subject_parent: sub_link, variable: 'transparent' %}{% endcapture %}{% if var == "true" %}{% if locksmith_b670_forloop__index == nil %}{% assign locksmith_b670_forloop__index = 1 %}{% assign locksmith_b670_forloop__index0 = 0 %}{% else %}{% assign locksmith_b670_forloop__index = locksmith_b670_forloop__index | plus: 1 %}{% assign locksmith_b670_forloop__index0 = locksmith_b670_forloop__index0 | plus: 1 %}{% endif %}{% if locksmith_b670_forloop__index == 1 %}{% assign locksmith_b670_forloop__first = true %}{% else %}{% assign locksmith_b670_forloop__first = false %}{% endif %}{% if locksmith_b670_forloop__index == locksmith_b670_forloop__size %}{% assign locksmith_b670_forloop__last = true %}{% else %}{% assign locksmith_b670_forloop__last = false %}{% endif %}{% assign locksmith_b670_forloop__rindex = locksmith_b670_forloop__size | minus: locksmith_b670_forloop__index | minus: 1 %}{% assign locksmith_b670_forloop__rindex0 = locksmith_b670_forloop__size | minus: locksmith_b670_forloop__index0 | minus: 1 %}{% else %}{% continue %}{% endif -%}
                                    {%- comment %}</locksmith:08dd>{% endcomment -%}
                                    <li class="Linklist__Item">
                                      <a href="{{ sub_sub_link.url }}" class="{% comment %}PRC EDITED{% endcomment %}{% if locksmith_b670_forloop__first and sub_sub_link.handle contains 'all' %}full-underline{% endif %}{% if locksmith_b670_forloop__last and link.handle contains 'apparel' %}full-underline{% endif %}{% comment %}END PRC EDITED{% endcomment %} Text--subdued Link Link--primary Link--sidenav final-link">{{ sub_sub_link.title | escape }}</a>
                                    </li>
                                  {%- endfor -%}
                                </ul>
                              {% comment %} </div> {% endcomment %}
                            {% comment %} </div> {% endcomment %}
                          {%- else -%}
                            <a href="{{ sub_link.url }}" class="Collapsible__Button Heading Link Link--primary u-h7 Link--sidenav final-link">{{ sub_link.title | escape }}</a>
                          {%- endif -%}
                        </div>
                      {%- endfor -%}
                    </div>
                  </div>
                {%- else -%}
                  <a href="{{ link.url }}" class="Collapsible__Button Heading Link Link--primary u-h6 Heading--sidenav heading-main">{{ link.title | escape }}</a>
                {%- endif -%}
              </div>
            {%- endfor -%}

