<script>
  var $thumbnails = $('.product-thumbnail-images.hidden-phone .product-thumbnail-image__img');
  var thumbnailCount = $thumbnails.length;
  console.log(thumbnailCount , 'thumbnailCount')
  var startX, startY;
  var swipeThreshold = 50; // Adjust this value to control the swipe sensitivity
  var swipeDirection = null; // Variable to track the swipe direction

  // generate dots
  for (var i = 0; i < thumbnailCount; i++) {
    var $dot = $('<div>').addClass('product-dot').attr('data-index', i);
    $('.product-dots').append($dot);
  }

  // activate first dot and thumbnail
  var firstIndex = 0; // Specify the index of the first thumbnail image
  $('.product-dot').eq(firstIndex).addClass('product-dot--active');
  $('.product-thumbnail-image__img').eq(firstIndex).parent().addClass('product-thumbnail-image--active');

  // set click event for thumbnails and dots
  $thumbnails.on('click', function() {
    var index = $(this).parent().data('index');
    updateImage(index);
  });

  $('.product-dots').on('click', '.product-dot', function() {
    var index = $(this).data('index');
    updateImage(index);
  });

  // add touch events for swipe
  var mainImage = $('.product-main-image__img')[0];
  mainImage.addEventListener('touchstart', touchStart, false);
  mainImage.addEventListener('touchmove', touchMove, false);
  mainImage.addEventListener('touchend', touchEnd, false);

  function touchStart(event) {
    var touch = event.touches[0];
    startX = touch.clientX;
    startY = touch.clientY;
  }

  function touchMove(event) {
    if (event.touches.length > 1) return;

    var touch = event.touches[0];
    var deltaX = touch.clientX - startX;
    var deltaY = touch.clientY - startY;

    // Check if swipe is horizontal or vertical
    if (Math.abs(deltaX) > Math.abs(deltaY)) {
      // Horizontal swipe
      event.preventDefault(); // Prevent scrolling while swiping

      if (deltaX > swipeThreshold) {
        // Swipe right
        swipeDirection = 'right';
      } else if (deltaX < -swipeThreshold) {
        // Swipe left
        swipeDirection = 'left';
      } else {
        swipeDirection = null;
      }
    } else {
      swipeDirection = null;
    }
  }

  function touchEnd(event) {
    if (swipeDirection === 'right') {
      var currentIndex = $('.product-dot--active').data('index');
      var newIndex = currentIndex === 0 ? thumbnailCount - 1 : currentIndex - 1;
      updateImage(newIndex);
    } else if (swipeDirection === 'left') {
      var currentIndex = $('.product-dot--active').data('index');
      var newIndex = currentIndex === thumbnailCount - 1 ? 0 : currentIndex + 1;
      updateImage(newIndex);
    }

    swipeDirection = null;
  }

  // function to update the main image, active thumbnail/dot, and reinitialize zoom
  function updateImage(index) {
    var $thumbnail = $thumbnails.eq(index);

    // change main image
    var mainImageSrc = $thumbnail.data('main-image');
    var mainImage = $('.product-main-image__img');
    mainImage.fadeOut(200, function() {
      mainImage.attr('src', mainImageSrc);
      mainImage.fadeIn(200, function() {
        // reinitialize zoom
        $('#ex1').trigger('zoom.destroy').zoom();
      });
    });

    // update active thumbnail and dot
    $('.product-thumbnail-image--active').removeClass('product-thumbnail-image--active');
    $thumbnail.parent().addClass('product-thumbnail-image--active');

    $('.product-dot--active').removeClass('product-dot--active');
    $('.product-dot').eq(index).addClass('product-dot--active');
  }
