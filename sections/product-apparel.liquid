<div id="pre-order-template">
    <div class="FirstMainSection"         
      style="background-color: {{ section.settings.light-mode-background-color }};
            {% if section.settings.light-mode-background-image %}
            background-image: url({{ section.settings.light-mode-background-image | img_url: 'master' }});
            {% endif %}
            background-repeat: no-repeat;
            margin-top: -40px;
            padding-top: 40px;
            background-size: cover;">
      
{%- capture section_settings -%}
  {
  "enableHistoryState": true,
  "templateSuffix": {{ product.template_suffix | json }},
  "showInventoryQuantity": {{ section.settings.show_inventory_quantity | json }},
  "stackProductImages": {{ section.settings.stack_images | json }},
  "showThumbnails": {{ section.settings.show_thumbnails | json }},
  "inventoryQuantityThreshold": {{ section.settings.inventory_quantity_threshold }},
  "showPriceInButton": {{ section.settings.show_price_in_button | json }},
  "enableImageZoom": {{ section.settings.enable_image_zoom | json }},
  "showPaymentButton": {{ section.settings.show_payment_button | json }},
  "useAjaxCart": {% if settings.cart_type == 'drawer' %}true{% else %}false{% endif %}
  }
{%- endcapture -%}

{%- include 'breadcrumbs' -%}

<div class="beam-me-up-scotty text-uppercase">
  <span>
    <img loading="lazy" style="width: 12px; margin-bottom: 2px;" src="data:image/svg+xml;utf8;base64," />
  </span>
  <span>back to top</span>
</div>

<section class="Product Product--{{ section.settings.image_size }} container" data-section-id="{{ section.id }}" data-section-type="product" data-section-settings='{{ section_settings }}'>
  <div class="FlexboxIeFix flex-wrap Product__Wrapper js-attach-heading">

    <!-- START: add review and rating container -->
    <div class="yotpo-product-banner mobile-yotpo">
      <div class="yotpo-widget-instance" data-yotpo-instance-id="498700" data-yotpo-product-id="{{product.id}}" data-yotpo-cart-product-id="{{item.product.id}}" data-yotpo-section-id="{{template.name}}"></div>
    </div>
    <!-- END: add review and rating container -->

    <div class="product-section">

      <div class="product-main-image">
       <div class="zoom-container">
          <span class="zoom" id="ex1">
            <img loading="lazy" src="{{ product.featured_image.src | img_url: 'master' }}" alt="{{ product.featured_image.alt | escape }}" class="product-main-image__img">
          </span>
        </div>
      </div>

      <div class="product-dots {% if product.metafields.meta.video != blank %}videoAvailable{% endif %}"></div>
  
      <div class="product-thumbnail-images owl-carousel owl-theme hidden-phone" data-template-suffix="{{ product.template_suffix }}" id="app-main">
        {% assign maxThumbnails = 10 %}
        {% for image in product.images %}
          {% if forloop.index <= maxThumbnails %}
            {% if forloop.index == 2 %}
              {% if product.metafields.meta.video != blank %}
                <div class="item">
                    <div class="product-thumbnail-image videoContainer">
                      <a class="product-video" href="{{ product.metafields.meta.video}}"></a>
                      <img loading="lazy" src="{{ product.images[0].src | img_url: 'master' }}" alt="{{ product.images[0].alt | escape }}" class="product-thumbnail-image__img" data-main-image="{{ product.images[0].src | img_url: 'master' }}">
                      {% comment %}{% include 'responsive-image' with image:  product.images[0], class='video-image' %}{% endcomment %}
                    </div>
                </div>
              {% endif %}
            {% endif %}
            <div class="item">
              <div class="product-thumbnail-image{% if forloop.first %} product-thumbnail-image--active{% endif %}" data-index="{{ forloop.index0 }}">
                <img loading="lazy" src="{{ image.src | img_url: 'master' }}" alt="{{ image.alt | escape }}" class="product-thumbnail-image__img" data-main-image="{{ image.src | img_url: 'master' }}">
              </div>
            </div>
          {% endif %}
        {% endfor %}
      </div>
      <div class="product-thumbnail-images owl-carousel owl-theme desktop-hidden" data-template-suffix="{{ product.template_suffix }}" id="app-main">
        {% assign maxThumbnails = 10 %}
        {% for image in product.images %}
          {% if forloop.index <= maxThumbnails %}
            {% if forloop.index == 2 %}
              {% if product.metafields.meta.video_mobile != blank %}
                <div class="item">
                    <div class="product-thumbnail-image videoContainer">
                      <a class="product-video" href="{{ product.metafields.meta.video_mobile}}"></a>
                      <img src="{{ product.images[0].src | img_url: 'master' }}" alt="{{ product.images[0].alt | escape }}" class="product-thumbnail-image__img" data-main-image="{{ product.images[0].src | img_url: 'master' }}">
                      {% comment %}{% include 'responsive-image' with image:  product.images[0], class='video-image' %}{% endcomment %}
                    </div>
                </div>
              {% endif %}
            {% endif %}
            <div class="item">
              <div class="product-thumbnail-image{% if forloop.first %} product-thumbnail-image--active{% endif %}" data-index="{{ forloop.index0 }}">
                <img src="{{ image.src | img_url: 'master' }}" alt="{{ image.alt | escape }}" class="product-thumbnail-image__img" data-main-image="{{ image.src | img_url: 'master' }}">
              </div>
            </div>
          {% endif %}
        {% endfor %}
      </div>


  
</div>

    <div class="Product__InfoWrapper">
      <div class="Product__Info product-page-info {% if image_count == 0 %}Product__Info--noGallery{% endif %}">
        <div class="Container">
          {% comment %}{%- include 'product-meta', show_description: true,blocks:section.blocks -%}{% endcomment %}
          {%- include 'apparel-product-meta', show_description: true,blocks:section.blocks -%}

          {% comment %}{%- include 'product-form-new' -%}{% endcomment %}

          {% comment %}{%- if section.settings.description_below_add_to_cart -%}
          <div class="ProductMeta__Description Rte">
            {{ product.description }}
          </div>
          {%- endif -%}{% endcomment %}

        </div>
      </div>
    </div>

  </div>

  {{- product_popovers -}}
  {{- frequent_product_popovers -}}
  {{- product_modals -}}
</section>

